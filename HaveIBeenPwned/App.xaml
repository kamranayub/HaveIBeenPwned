<Application
    x:Class="HaveIBeenPwned.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:haveIBeenPwned="clr-namespace:HaveIBeenPwned"
    xmlns:micro="clr-namespace:Caliburn.Micro;assembly=Caliburn.Micro"
    xmlns:converters="clr-namespace:HaveIBeenPwned.Converters">

    <!--Application Resources-->
    <Application.Resources>
        <local:LocalizedStrings xmlns:local="clr-namespace:HaveIBeenPwned" x:Key="LocalizedStrings"/>
        <haveIBeenPwned:AppBootstrapper x:Key="bootstrapper" />
        <micro:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
        <converters:ObjectToVisibilityConverter x:Key="ObjectToVisibilityConverter"/>
        <converters:NotBooleanToVisibilityConverter x:Key="NotBooleanToVisibilityConverter"/>
    </Application.Resources>

</Application>